    "test": "mocha --check-leaks --timeout=10000 ./test/*.spec.js"
  },
  "dependencies": {
    "@types/istanbul-lib-coverage": "2",
    "@types/node": ">=14",
    "c8": "7",
    "signal-exit": "3",
    "which": "2"
  },
  "devDependencies": {
    "@fasttime/lint": "latest",
    "mocha": "latest",
    "ts-node": "latest",
    "typescript": "latest"
  },
  "engines": {
    "node": ">=14"
  },
  "exports": {
    ".": "./lib/c8js.js",
    "./package.json": "./package.json"
  },
  "type": "module",
  "types": "lib/c8js.d.ts"
}
